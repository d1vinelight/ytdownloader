from pytube import YouTube 

link = 'https://www.youtube.com/watch?v=TwkNBJvaDyk&list=PLx40Tc4pO423OvwMLI7VNwXqoSOnWF_53&index=3'

def download_video():
    yt = YouTube(link)
    path = yt.streams.filter(file_extension = 'mp4').\
        get_highest_resolution().download()
    print(path)
    return '--Video was downloaded succsessfully--'
    
def main():
    download_video()
    
if __name__ == '__main__':
    main()
